{
  "name": "af2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "tsc": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "check-all": "bun run type-check && bun run lint && bun run format:check"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-table": "^8.21.3",
    "framer-motion": "^12.23.12",
    "lottie": "^0.0.1",
    "lottie-react": "^2.4.1",
    "next": "15.4.1",
    "ngl": "^2.4.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-fast-marquee": "^1.6.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/chroma-js": "^3.1.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/signals": "^1.0.4",
    "@types/three": "^0.179.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}